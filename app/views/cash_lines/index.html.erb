<%= content_for :menu do %>
  <%= icon_to 'retour.png', :back, title: 'Retour' %>
<% end -%>


<fieldset><legend>Livre de caisse - <%=  @book.name %></legend>
  <div id="submenu">
    <span><%=  link_to_if @period.previous_period?, '<<', change_organism_period_path(@organism,@period.previous_period) %></span>
    <%= cashsubmenu_helper(@book, @period) %>
    <span><%= link_to_if @period.next_period? ,'>>', change_organism_period_path(@organism,@period.next_period) %></span>
  </div>

  <table class="data_table display">
    <thead>
      <tr>
        <%= render 'table_header' %>
      </tr>
    </thead>
    <tbody id="table_lines">
      <%= render 'table_lines'  %>
    </tbody>
    <tfoot id="table_foot">

      <%= render 'table_foot' %>
    </tfoot>

  </table>

</fieldset>

