.home
  - unless controller_name == 'registrations'
    = render 'layouts/upper_menu' if @organism && @organism.persisted?
  - if @organism && @organism.persisted?
    = link_to header_title, organism_path(@organism), :tabindex=>-1, id:'home', class:'with_spinner'
  -else
    = header_title
  = render 'layouts/login_logout'
  